    private void btRetrieveActionPerformed(java.awt.event.ActionEvent evt) {                                           
try{   
   stmt =conn.createStatement();
   String index = txtIndex.getText();
   String name = txtName.getText();
   String sub1 = txtSub1.getText();
   String sub2 = txtSub2.getText();
   String sub3 = txtSub3.getText();
   double gpa= Double.parseDouble(txtgpa.getText());
    
    String sql = "INSERT INTO studentdetails.stddetails (IndexNumber,StdName,Subject1,Subject2,Subject3,GPA) VALUES('"+index+"','"+name+"','"+sub1+"','"+sub2+"','"+sub3+"','"+gpa+"')";
     stmt.executeUpdate(sql);
    JOptionPane.showMessageDialog(null, "Value added Successfully");

// TODO add your handling code here:
    }                                          
catch(Exception e){
    JOptionPane.showMessageDialog(null,e);
}
    }
